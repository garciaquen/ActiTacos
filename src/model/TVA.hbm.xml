<?xml version="1.0"?><!DOCTYPE hibernate-mapping
        PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="model.TVA" table="TVA">
        <id column="id_TVA" name="id_TVA" type="int">
            <generator class="identity"/>
        </id>

        <property name="taux" type="java.lang.Float" column="TAUX" not-null="true"/>
        <property name="compte_Achat" type="java.lang.String" column="COMPTE_ACHAT"/>
        <property name="compte_Immo" type="java.lang.String" column="COMPTE_IMMO"/>
        <property name="nom" type="java.lang.String" column="NOM" not-null="true"/>


    </class>
</hibernate-mapping>